    "email": "martin8900@gmail.com"
  },
  "scripts": {},
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.2.3",
    "connect-history-api-fallback": "0.0.5",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-cdnizer": "^1.1.4",
    "gulp-eslint": "^0.5.0",
    "gulp-flatten": "0.0.4",
    "gulp-include-source": "0.0.5",
    "gulp-inline-angular-templates": "^0.1.5",
    "gulp-less": "^3.0.1",
    "gulp-minify-css": "^0.5.1",
    "gulp-minify-html": "^1.0.0",
    "gulp-processhtml": "^1.0.5",
    "gulp-rev": "^3.0.1",
    "gulp-uglify": "^1.1.0",
    "gulp-usemin": "^0.3.11",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-html-reporter": "^0.1.6",
    "karma-opera-launcher": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.16",
    "run-sequence": "^1.0.2",
    "wiredep": "^2.2.2"
  },
  "eslintConfig": {
    "rules": {
      "quotes": "single",
      "no-use-before-define": false,
      "eqeqeq": false,
      "camelcase": false,
      "no-console": true,
      "no-extra-parens": true,
      "no-else-return": true,
      "no-self-compare": true,
      "no-void": true,
      "no-warning-comments": true,
      "no-underscore-dangle": false,
      "wrap-iife": true,
      "global-strict": [
        2,
        "never"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "consistent-this": [
        0,
        "that"
      ],
      "space-in-brackets": [
        2,
        "never"
      ],
      "space-in-parens": [
        2,
        "never"
      ],
      "space-unary-word-ops": true,
      "spaced-line-comment": true
    }
  }
}
